{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "name": "Z Combinator API",
  "colors": {
    "primary": "#b2e9fe",
    "light": "#d4f2ff",
    "dark": "#7ac8e6"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guide",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "configuration"
            ]
          },
          {
            "group": "Integration",
            "pages": [
              "integration/overview",
              "integration/examples",
              "integration/github-discord-webhook"
            ]
          },
          {
            "group": "Error Handling",
            "pages": [
              "errors/overview",
              "errors/status-codes"
            ]
          },
          {
            "group": "Governance",
            "pages": [
              "governance",
              "addresses"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Launch endpoints",
            "pages": [
              "api-reference/launch/create",
              "api-reference/launch/confirm"
            ]
          },
          {
            "group": "Claims endpoints",
            "pages": [
              "api-reference/claims/eligibility",
              "api-reference/claims/mint",
              "api-reference/claims/confirm"
            ]
          },
          {
            "group": "Utility endpoints",
            "pages": [
              "api-reference/utility/health",
              "api-reference/utility/verify-token"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://zcombinator.io",
          "icon": "globe"
        },
        {
          "anchor": "Twitter",
          "href": "https://x.com/zcombinatorio",
          "icon": "x-twitter"
        },
        {
          "anchor": "Support",
          "href": "https://x.com/zcombinatorio",
          "icon": "headset"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/zcombinatorio/zcombinator",
          "icon": "github"
        }
      ]
    }
  },
  "logo": "/logo/zcombinator-logo.png",
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://x.com/zcombinatorio"
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/zcombinatorio",
      "github": "https://github.com/zcombinatorio/zcombinator"
    }
  },
  "css": "/custom.css"
}
